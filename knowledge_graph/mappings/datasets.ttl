@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix meh: <http://tools.mehdie.org/ns/> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#DatasetMapping>

  rml:logicalSource [
    rml:source "./database_dump/datasets.csv" ;
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [
    rr:template "http://tools.mehdie.org/ns/datasets/{id}" ;
    rr:class meh:Dataset
  ];

  rr:predicateObjectMap [
    rr:predicate meh:label ;
    rr:objectMap [
       rml:reference "label";
       rml:datatype xsd:string
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate meh:title ;
    rr:objectMap [
       rml:reference "title";
       rml:datatype xsd:string
    ]
  ].